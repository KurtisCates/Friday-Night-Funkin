		switch (curStage)
		{
			case 'limo':
				player1.y -= 220;
				player1.x += 260;

				resetFastCar();
				add(fastCar);

			case 'mall':
				player1.x += 200;

			case 'mallEvil':
				player1.x += 320;
				player2.y -= 80;
			case 'school':
				player1.x += 200;
				player1.y += 220;
				gf.x += 180;
