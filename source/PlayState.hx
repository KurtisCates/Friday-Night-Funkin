					stageCurtains.antialiasing = true;
					stageCurtains.scrollFactor.set(1.3, 1.3);
					stageCurtains.active = false;

					add(stageCurtains);
				}
		}

		gf = new Character(400, 130, SONG.gf);
		gf.scrollFactor.set(0.95, 0.95);

		player2 = new Character(100, 100, SONG.player2);

		var camPos:FlxPoint = player2.getGraphicMidpoint();

		switch (SONG.player2)
		{
			case 'gf' | 'gf-pixel' | 'gf-christmas':
				player2.setPosition(gf.x, gf.y);
				gf.visible = false;
				if (isStoryMode)
				{
					camPos.x += 600;
					tweenCamIn();
				}
			case "spooky":
				player2.y += 200;
			case "monster":
				player2.y += 100;
			case 'monster-christmas':
				player2.y += 130;
			case 'dad':
				camPos.x += 400;
			case 'pico':
				camPos.x += 600;
				player2.y += 300;
			case 'parents-christmas':
				player2.x -= 500;
			case 'senpai' | 'senpai-angry':
				player2.x += 150;
				player2.y += 360;
				var mid = player2.getGraphicMidpoint();
				camPos.set(mid.x + 300, mid.y);
			case 'spirit':
				player2.x -= 150;
				player2.y += 100;
				var mid = player2.getGraphicMidpoint();
				camPos.set(mid.x + 300, mid.y);
		}

